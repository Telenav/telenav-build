use 5.018;
use strict;
use warnings;

require "$ENV{'TELENAV_WORKSPACE'}/bin/telenav-functions.pl";

my $branch_name = get_argument("Branch name? ");

if (!defined $branch_name)
{
    fail_with_usage("[branch-name]");
}

system("telenav-checkout bugfix/$branch_name");
